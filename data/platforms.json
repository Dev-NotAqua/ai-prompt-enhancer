{
  "chatgpt": {
    "name": "ChatGPT",
    "domains": ["chat.openai.com"],
    "selectors": {
      "input": [
        "textarea[data-id]",
        "#prompt-textarea",
        "textarea[placeholder*=\"message\"]",
        "[contenteditable=\"true\"][data-id]",
        "textarea"
      ],
      "textarea": [
        "textarea[data-id]",
        "textarea[placeholder*=\"message\"]",
        "textarea"
      ],
      "messages": [
        "[data-message-author-role]",
        ".group",
        "[data-testid*=\"conversation-turn\"]",
        ".text-base"
      ],
      "userMessage": [
        "[data-message-author-role=\"user\"]",
        ".bg-gray-50"
      ],
      "assistantMessage": [
        "[data-message-author-role=\"assistant\"]",
        ".group:not(.bg-gray-50)"
      ],
      "sendButton": [
        "[data-testid=\"send-button\"]",
        "button[aria-label*=\"Send\"]",
        "button svg[data-testid=\"send-button\"]"
      ],
      "chatContainer": [
        "main",
        "[role=\"main\"]",
        ".conversation-container"
      ]
    },
    "features": {
      "supportsMarkdown": true,
      "supportsCodeBlocks": true,
      "supportsFileUpload": true,
      "maxPromptLength": 32000,
      "hasConversationHistory": true,
      "supportsContinuation": true
    },
    "detection": {
      "urlPatterns": [
        "https://chat.openai.com/*"
      ],
      "requiredElements": [
        "textarea",
        "[data-testid=\"send-button\"]"
      ]
    },
    "styling": {
      "primaryColor": "#10a37f",
      "backgroundColor": "#ffffff",
      "textColor": "#343541",
      "darkMode": {
        "backgroundColor": "#343541",
        "textColor": "#ffffff"
      }
    }
  },
  "claude": {
    "name": "Claude (Anthropic)",
    "domains": ["claude.ai"],
    "selectors": {
      "input": [
        "[contenteditable=\"true\"]",
        "textarea",
        ".ProseMirror",
        "[data-testid=\"chat-input\"]"
      ],
      "textarea": [
        "textarea",
        "[contenteditable=\"true\"]"
      ],
      "messages": [
        "[data-testid*=\"message\"]",
        ".message",
        "[data-is-from-user]",
        ".font-user-message"
      ],
      "userMessage": [
        "[data-is-from-user=\"true\"]",
        ".human-message",
        ".user-message"
      ],
      "assistantMessage": [
        "[data-is-from-user=\"false\"]",
        ".assistant-message",
        ".claude-message"
      ],
      "sendButton": [
        "button[aria-label*=\"Send\"]",
        "[data-testid=\"send-button\"]",
        "button[type=\"submit\"]"
      ],
      "chatContainer": [
        "main",
        ".chat-container",
        "[data-testid=\"chat-container\"]"
      ]
    },
    "features": {
      "supportsMarkdown": true,
      "supportsCodeBlocks": true,
      "supportsFileUpload": true,
      "maxPromptLength": 100000,
      "hasConversationHistory": true,
      "supportsContinuation": true,
      "supportsArtifacts": true
    },
    "detection": {
      "urlPatterns": [
        "https://claude.ai/*"
      ],
      "excludePatterns": [
        "*/login",
        "*/auth"
      ],
      "requiredElements": [
        "[contenteditable=\"true\"]"
      ]
    },
    "styling": {
      "primaryColor": "#cc785c",
      "backgroundColor": "#ffffff",
      "textColor": "#1f1f1f",
      "darkMode": {
        "backgroundColor": "#1f1f1f",
        "textColor": "#ffffff"
      }
    }
  },
